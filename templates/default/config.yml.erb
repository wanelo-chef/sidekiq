---
:verbose: <%= @verbose %>
:namespace: <%= @namespace %>
:concurrency: <%= @concurrency %>
:processes: <%= @processes %>
:pid_file: <%= @pid_dir %>/%s-%s.pid
:timeout: <%= @sidekiq_timeout %>
:queues:
<% @queues.each do |queue_name, priority| %>
  - [<%= queue_name %>, <%= priority %>]
<% end %>
